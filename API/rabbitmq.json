{
    "$schema": "./node_modules/rascal/lib/config/schema.json",
    "vhosts": {
        "v1": {
            "connection": {
                "slashes": true,
                "protocol": "amqp",
                "hostname": "localhost",
                "user": "guest",
                "password": "guest",
                "port": 5672,
                "vhost": "v1",
                "options": {
                    "heartbeat": 5
                },
                "socketOptions": {
                    "timeout": 10000
                }
            }
        }
    },
    "exchanges": [
        "demo_ex"
    ],
    "queues": [
        "demo_q"
    ],
    "bindings": [
        "demo_ex[a.b.c] -> demo_q"
    ],
    "publications": {
        "demo_pub": {
            "exchange": "demo_ex",
            "routingKey": "a.b.c"
        }
    },
    "subscriptions": {
        "demo_sub": {
            "queue": "demo_q",
            "prefetch": 3
        }
    }
}

